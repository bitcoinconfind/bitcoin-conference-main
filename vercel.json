{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "bitcoinconferenceindia.com" }
      ],
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.bitcoinconferenceindia.com" }
      ],
      "destination": "https://bitcoinconferenceindia.com/:path*",
      "permanent": true
    }
  ]
}


